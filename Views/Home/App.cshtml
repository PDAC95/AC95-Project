@{
    ViewData["Title"] = "App";
}

<div class="mil-content">
    <div id="swupMain" class="mil-main-transition">

        <!-- banner -->
        <div class="mil-inner-banner">
            <div class="mil-animation-frame">
                <div class="mil-animation mil-position-4 mil-dark mil-scale" data-value-1="6" data-value-2="1.4"></div>
            </div>
            <div class="mil-banner-content mil-up">
                <div class="container">
                    <ul class="mil-breadcrumbs mil-mb-60">
                        <li><a asp-controller="Home" asp-action="Home">Homepage</a></li>
                        <li><a asp-controller="Home" asp-action="AppDevelopment">App Development</a></li>
                    </ul>
                    <h1 class="mil-mb-60">Innovative <span class="mil-thin">App</span><br> Development for <span class="mil-thin">Your Business</span></h1>
                    <a href="#service" class="mil-link mil-dark mil-arrow-place mil-down-arrow">
                        <span>Get Started</span>
                    </a>
                </div>
            </div>
        </div>
        <!-- banner end -->
        <!-- service -->
        <section id="service">
            <div class="container mil-p-120-90">
                <div class="row justify-content-between">
                    <div class="col-lg-4 mil-relative mil-mb-90">

                        <h4 class="mil-up mil-mb-30">Our <span class="mil-thin">App</span> <br>Development <span class="mil-thin">Services</span></h4>
                        <p class="mil-up mil-mb-30">At AC95, we offer comprehensive app development services that cater to businesses looking to innovate. From mobile apps to web applications, we build solutions that provide outstanding user experiences and drive business growth.</p>
                        <div class="mil-up">
                            <a href="services.html" class="mil-link mil-dark mil-arrow-place">
                                <span>Start Your Project</span>
                            </a>
                        </div>

                    </div>
                    <div class="col-lg-6">

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">iOS App Development</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">We create high-performance, feature-packed iOS applications designed to engage users and deliver seamless experiences. From concept to launch, we ensure your iOS app stands out in the App Store.</p>
                            </div>
                        </div>

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">Android App Development</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">Our team builds powerful Android apps that deliver excellent performance and user engagement. We develop apps tailored to the needs of your business, ensuring compatibility across a range of devices and OS versions.</p>
                            </div>
                        </div>

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">Cross-Platform App Development</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">We specialize in cross-platform app development using frameworks like Xamarin and React Native. This allows us to create apps that work seamlessly on both iOS and Android, saving time and development costs while maintaining a high-quality user experience.</p>
                            </div>
                        </div>

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">Web Application Development</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">Our web application development services focus on building high-performing, scalable web apps that provide a seamless experience across browsers and devices. Whether it's a progressive web app (PWA) or a custom web solution, we deliver results that enhance user interaction and business efficiency.</p>
                            </div>
                        </div>

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">App Maintenance and Support</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">We provide ongoing maintenance and support services to keep your app running smoothly. From regular updates to bug fixes and feature enhancements, we ensure your app remains relevant and functional in a competitive market.</p>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </section>
        <!-- service end -->
    </div>
</div>

