@{
    ViewData["Title"] = "Ecommerce";
}

<div class="mil-content">
    <div id="swupMain" class="mil-main-transition">

        <!-- banner -->
        <div class="mil-inner-banner">
            <div class="mil-animation-frame">
                <div class="mil-animation mil-position-4 mil-dark mil-scale" data-value-1="6" data-value-2="1.4"></div>
            </div>
            <div class="mil-banner-content mil-up">
                <div class="container">
                    <ul class="mil-breadcrumbs mil-mb-60">
                        <li><a asp-controller="Home" asp-action="Home">Homepage</a></li>
                        <li><a asp-controller="Home" asp-action="Ecommerce">E-Commerce</a></li>
                    </ul>
                    <h1 class="mil-mb-60">Building <span class="mil-thin">E-Commerce</span><br> Solutions for <span class="mil-thin">Your Business</span></h1>
                    <a href="#service" class="mil-link mil-dark mil-arrow-place mil-down-arrow">
                        <span>Get Started</span>
                    </a>
                </div>
            </div>
        </div>
        <!-- banner end -->
        <!-- service -->
        <section id="service">
            <div class="container mil-p-120-90">
                <div class="row justify-content-between">
                    <div class="col-lg-4 mil-relative mil-mb-90">

                        <h4 class="mil-up mil-mb-30">Our <span class="mil-thin">E-Commerce</span> <br>Development <span class="mil-thin">Services</span></h4>
                        <p class="mil-up mil-mb-30">At AC95, we specialize in creating powerful, user-friendly e-commerce solutions that drive sales and enhance customer experiences. From product listings to secure payments, we ensure that your store is built for success.</p>
                        <div class="mil-up">
                            <a href="services.html" class="mil-link mil-dark mil-arrow-place">
                                <span>Start Your Project</span>
                            </a>
                        </div>

                    </div>
                    <div class="col-lg-6">

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">Custom E-Commerce Development</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">We develop fully customized e-commerce platforms tailored to your business needs. From product catalogs to shopping carts and payment gateways, our solutions are designed to provide a seamless shopping experience.</p>
                            </div>
                        </div>

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">Responsive E-Commerce Design</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">Our e-commerce stores are built with responsive design, ensuring they work seamlessly across all devices, from mobile to desktop. This guarantees a smooth shopping experience for your customers, no matter how they browse.</p>
                            </div>
                        </div>

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">Secure Payment Integration</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">We integrate secure payment gateways that allow customers to make payments easily and safely. Our e-commerce solutions support popular payment methods such as credit cards, PayPal, and more.</p>
                            </div>
                        </div>

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">Inventory Management</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">Our systems include inventory management features that help you track stock levels, manage product listings, and update prices in real time, ensuring your online store is always accurate and up to date.</p>
                            </div>
                        </div>

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">Performance and SEO Optimization</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">We optimize your e-commerce site for fast loading times and high search engine rankings. Our SEO-focused development helps drive organic traffic and increases your chances of converting visitors into customers.</p>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </section>
        <!-- service end -->
    </div>
</div>
