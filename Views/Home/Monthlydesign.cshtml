@{
    ViewData["Title"] = "Monthlydesign";
}

<div class="mil-content">
    <div id="swupMain" class="mil-main-transition">

        <!-- banner -->
        <div class="mil-inner-banner">
            <div class="mil-animation-frame">
                <div class="mil-animation mil-position-4 mil-dark mil-scale" data-value-1="6" data-value-2="1.4"></div>
            </div>
            <div class="mil-banner-content mil-up">
                <div class="container">
                    <ul class="mil-breadcrumbs mil-mb-60">
                        <li><a asp-controller="Home" asp-action="Home">Homepage</a></li>
                        <li><a asp-controller="Home" asp-action="Monthly">Monthly Plans</a></li>
                        <li><a asp-controller="Home" asp-action="Monthlydesign">Graphic Design</a></li>
                    </ul>
                    <h1 class="mil-mb-60">Monthly <span class="mil-thin">Graphic</span><br> Design <span class="mil-thin">Services</span></h1>
                    <a href="#service" class="mil-link mil-dark mil-arrow-place mil-down-arrow">
                        <span>Get Started</span>
                    </a>
                </div>
            </div>
        </div>
        <!-- banner end -->
        <!-- service -->
        <section id="service">
            <div class="container mil-p-120-90">
                <div class="row justify-content-between">
                    <div class="col-lg-4 mil-relative mil-mb-90">

                        <h4 class="mil-up mil-mb-30">Our <span class="mil-thin">Monthly</span> <br>Graphic Design <span class="mil-thin">Services</span></h4>
                        <p class="mil-up mil-mb-30">Our graphic design subscription service provides you with ongoing design support every month. Whether you need logos, marketing materials, or social media graphics, our team delivers high-quality designs tailored to your brand.</p>
                        <div class="mil-up">
                            <a href="payment.html" class="mil-link mil-dark mil-arrow-place">
                                <span>Subscribe Now</span>
                            </a>
                        </div>

                    </div>
                    <div class="col-lg-6">

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">Custom Graphic Design</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">Get custom graphic design solutions tailored to your brand's needs. From logos to business cards and product packaging, we create designs that enhance your visual identity and make a lasting impact.</p>
                            </div>
                        </div>

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">Marketing Materials</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">We design eye-catching marketing materials like brochures, flyers, and banners to help you promote your business. Our designs are created to grab attention and drive engagement with your audience.</p>
                            </div>
                        </div>

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">Social Media Graphics</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">Our team creates engaging social media graphics to boost your online presence. From Instagram posts to Facebook banners, we design content that aligns with your brand and engages your followers.</p>
                            </div>
                        </div>

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">Brand Identity Design</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">We help businesses develop a strong visual identity by designing logos, color schemes, and typography that reflect your brand's personality and values. Our brand identity designs set you apart from the competition.</p>
                            </div>
                        </div>

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">Print & Digital Assets</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">From digital banners to printed materials, we provide designs that work across all formats. Whether for online ads, print campaigns, or trade show materials, we create assets that are visually stunning and effective.</p>
                            </div>
                        </div>

                        <div class="mil-accordion-group mil-up">
                            <div class="mil-accordion-menu">
                                <p class="mil-accordion-head">Unlimited Design Requests</p>

                                <div class="mil-symbol mil-h3">
                                    <div class="mil-plus">+</div>
                                    <div class="mil-minus">-</div>
                                </div>
                            </div>
                            <div class="mil-accordion-content">
                                <p class="mil-mb-30">Our monthly service allows you to request unlimited design revisions and new designs. We work with you continuously to refine and create designs that meet your evolving business needs.</p>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </section>
        <!-- service end -->
    </div>
</div>

